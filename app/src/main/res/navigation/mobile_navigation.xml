<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.pccsmartapp.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_list_anggota"
        android:name="com.example.pccsmartapp.ui.listanggota.ListAnggotaFragment"
        android:label="List Anggota"
        tools:layout="@layout/fragment_list_anggota" />

    <fragment
        android:id="@+id/navigation_list_event"
        android:name="com.example.pccsmartapp.ui.listevent.ListEventFragment"
        android:label="List Event"
        tools:layout="@layout/fragment_list_event" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.pccsmartapp.ui.profile.ProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_profile" />
</navigation>